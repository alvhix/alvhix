---
interface Props {
  type: IconName;
}

const { type } = Astro.props;

export enum IconName {
  TRAILHEAD = 'salesforce.svg',
  TWITTER = 'twitter.svg',
  GITHUB = 'github.svg',
  RSS = 'rss.svg',
}
---

<span class="slds-icon">
  <img src={`/svg/${type}`} alt={type.replace('.svg', '')} />
</span>
